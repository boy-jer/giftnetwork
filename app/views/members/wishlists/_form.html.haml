= simple_form_for(@wishlist) do |f|
  - if @wishlist.errors.any?
    #error_explanation
      %h2
        = pluralize(@wishlist.errors.count, "error")
        prohibited this wishlist from being saved:

      %ul
        - @wishlist.errors.full_messages.each do |msg|
          %li= msg

  .inputs
    = f.input :member_id
    = f.input :product_origin
    = f.input :product_identifier

  .actions
    = f.button :submit
